<%= form_for(customer) do |customer| %>
<div>
  <p><%= link_to 'Edit', edit_customer_path(customer) %> <%= link_to 'x', customer, method: :delete, data: { confirm: 'Are you sure?' } %></p>
  <p><%= customer.full_name %></p>
  <%= image_tag(customer.image) %>
  <p><%= customer.phone_number %></p>
  <p><%= customer.email_address %></p>
  <p><%= customer.image %></p>
  <p><%= customer.notes %></p>
</div>
